/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=b,out=notab);
	Nand(a=a,b=notab,out=out1);
	Nand(a=notab,b=b,out=out2);
	Nand(a=out1,b=out2,out=out);
}